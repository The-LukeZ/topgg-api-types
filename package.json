{
  "name": "topgg-api-types",
  "author": {
    "name": "The-LukeZ",
    "email": "contact.lukez@proton.me",
    "url": "https://github.com/The-LukeZ"
  },
  "type": "module",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Top.gg API types and validators for TypeScript",
  "repository": {
    "url": "https://github.com/The-LukeZ/topgg-api-types"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./v0": {
      "require": "./dist/v0.cjs",
      "import": "./dist/v0.js"
    },
    "./v1": {
      "require": "./dist/v1.cjs",
      "import": "./dist/v1.js"
    },
    "./v1/validators": {
      "require": "./dist/v1/validators.cjs",
      "import": "./dist/v1/validators.js"
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "topgg",
    "types",
    "typescript",
    "api"
  ],
  "scripts": {
    "prepublishOnly": "pnpm run build",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "format": "prettier --write .",
    "lint": "prettier --check ."
  },
  "devDependencies": {
    "prettier": "3.8.1",
    "tsdown": "^0.18.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "zod": "^4.3.6"
  }
}
